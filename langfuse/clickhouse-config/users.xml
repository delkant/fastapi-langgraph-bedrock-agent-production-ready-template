<?xml version="1.0"?>
<clickhouse>
    <!-- Profiles must be defined before users that reference them -->
    <profiles>
        <default>
            <max_memory_usage>10000000000</max_memory_usage>
            <use_uncompressed_cache>0</use_uncompressed_cache>
            <load_balancing>random</load_balancing>
            <max_threads>8</max_threads>
            <max_query_size>268435456</max_query_size>
            <interactive_delay>100000</interactive_delay>
            <max_block_size>65536</max_block_size>
            <max_insert_block_size>1048576</max_insert_block_size>
            <log_queries>1</log_queries>
        </default>
    </profiles>

    <!-- Quotas must be defined before users that reference them -->
    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>

    <!-- Users section comes last -->
    <users>
        <!-- Default user for local development -->
        <default>
            <password></password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>

        <!-- Langfuse user -->
        <langfuse>
            <password>langfuse_ch_password</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </langfuse>
    </users>
</clickhouse>